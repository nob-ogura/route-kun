{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build", "^generate", "generate", "^typecheck", "typecheck"],
      "outputs": [
        "dist/**",
        ".next/**"
      ],
      "cache": true
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "typecheck": {
      "dependsOn": ["^generate", "generate", "^typecheck"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["^typecheck", "typecheck"],
      "outputs": []
    },
    "e2e": {
      "dependsOn": ["^build", "build"],
      "outputs": []
    },
    "generate": {
      "outputs": ["src/**", "types/**"]
    }
  }
}
