{
  "name": "route-kun",
  "private": true,
  "version": "0.0.0",
  "description": "Monorepo for route-kun (apps + packages)",
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.20.0",
  "scripts": {
    "install:repo": "pnpm install",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "typecheck": "turbo run typecheck",
    "lint": "turbo run lint",
    "test": "turbo run test e2e",
    "e2e": "turbo run e2e",
    "generate": "turbo run generate"
  },
  "devDependencies": {
    "turbo": "^2.0.6",
    "typescript": "^5.6.3"
  }
}
